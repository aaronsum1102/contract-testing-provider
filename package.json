{
  "name": "provider",
  "version": "0.0.1",
  "description": "Backend service using Serverless Framework",
  "main": "index.js",
  "scripts": {
    "build": "sls package",
    "dev": "sls offline start",
    "test": "jest",
    "pact:can-i-deploy": "dotenv -- cross-var pact-broker can-i-deploy -b %PACT_BROKER_BASE_URL% -k %PACT_BROKER_TOKEN%",
    "pact:describe-version": "dotenv -- cross-var pact-broker describe-version -b %PACT_BROKER_BASE_URL% -k %PACT_BROKER_TOKEN%"
  },
  "keywords": [
    "backend",
    "lambda",
    "serverless",
    "node"
  ],
  "author": "Shao Xiang Sum",
  "license": "UNLICENSED",
  "dependencies": {
    "lambda-log": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@pact-foundation/pact": "^9.15.3",
    "@types/jest": "^26.0.22",
    "@types/lambda-log": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "aws-sdk": "^2.873.0",
    "babel-loader": "^8.2.2",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "serverless": "^2.31.0",
    "serverless-iam-roles-per-function": "^3.1.0",
    "serverless-offline": "^6.9.0",
    "serverless-webpack": "^5.4.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,mjs,ts}": [
      "prettier --write"
    ]
  },
  "config": {
    "pact_do_not_track": true
  }
}
